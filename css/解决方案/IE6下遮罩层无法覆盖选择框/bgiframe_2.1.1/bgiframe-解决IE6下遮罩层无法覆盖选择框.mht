/********************************************************************************

 * CoolCode Style

 *******************************************************************************/

.hl-title {
    background-color: #F9FBFC;
    border: 1px solid #C3CED9;
    border-bottom: 0;
    padding: 6px 24px;
    margin: 0;
    width: auto;
    line-height: 180%;
    overflow : auto;
    text-align: left;
    font-size: 14px;
    font-weight: bold;
}
.hl-surround {
    margin:15px 0;
    overflow : hidden;
    text-align: left;
    font-family: Fixedsys, "BitStream Vera Sans Mono", "Courier New", Courier, monospace;
}
.single-post .entry-content ol.hl-main {
    background-color: #DEFAD2;
    border-radius: 10px 10px 10px 10px;
    color: #FFFFFF;
    font-size: 12px;
    margin: 0;
    padding: 6px;
}
.single-post .entry-content ol.hl-main li{
    line-height:24px;
    text-indent:10px;
}
.single-post .entry-content ol.hl-main li.hl-firstline{
    border-top:none;
}
.single-post .entry-content .ln-show li {
    list-style: decimal outside;

}
li.hl-firstline {
    border-top: 0 !important;
}
