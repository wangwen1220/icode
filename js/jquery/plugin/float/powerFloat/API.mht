p,form,ul,li,ol{margin:0; padding:0; list-style-type:none;}
img{border:0; max-width:100%;}
.clear{clear:both;}
.center{text-align:center;}
.hidden{display:none;}
hr{display:none;}
.ml2{margin-left:2px;}
.ml5{margin-left:5px;}
.ml10{margin-left:10px;}
.ml20{margin-left:20px;}
