.params_table td{padding:2px 4px; border:3px solid #fff; background:#f9f9f9;}
.params_table tr:hover{opacity:0.8; filter:Alpha(opacity=80);}
span.s{font-size:0.9em; color:#999;}
/*slide change*/.widget_categories,.widget_archive li,.widget_archive .widgettitle{width:48.2%; float:right;}
.widget_recent_entries{clear:both; padding-top:15px;}
