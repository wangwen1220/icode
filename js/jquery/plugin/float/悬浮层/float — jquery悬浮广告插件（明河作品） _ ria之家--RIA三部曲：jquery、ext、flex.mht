			cfm.parentNode.removeChild(cfm);
			c.parentNode.insertBefore(cfm,c);
		}

		if(parseInt(dp) && Commentarea && Commentarea.display != "none"){
			Commentarea.focus();
			//Commentarea.value = '';
		}
	}
	return true;
}

