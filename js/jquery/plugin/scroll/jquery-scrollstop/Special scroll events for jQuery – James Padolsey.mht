/*
    Theme Name: James Padolsey Custom
    Theme URI: http://james.padolsey.com
    Description: Custom theme for james.padolsey.com
*/

body,img,ul,ol,dl,dt,dd,blockquote,p,h1,h2,h3,h4,h5,h6,form,fieldset {border: none;list-style: none; margin: 0;padding: 0;font-weight: normal; outline:none;}
body {background: #000 url(images/bg_dark.png); color: #FFF; font-size:  0.8em; font-family: Verdana, Helvetica, Arial, Sans-Serif;}
a {color: #FFF;text-decoration: none;}

.center {text-align: center;}
.inline {display: inline;}
.gap {font-size: 0.8em;}
code{ padding:0 4px; background: #141414; }
code, pre { font-family: "Bitstream Vera Sans Mono", "Consolas", "Lucida Console", monospace, mono !important; font-size: 1em !important; }

#header { background: #111 url(images/bg_falling_dark_trans.png); text-shadow: 1px 0 #FFF; text-transform: uppercase; line-height: 30px; border-bottom: 1px solid #111; width: 750px; margin: 0 auto; padding: 10px; overflow: hidden; }
#logo { float: left; font-size: 1.2em; padding-left: 10px; text-shadow: 0 0 2px #555; }
#nav { float: right; height: 30px; border-right: 1px solid #555; font-size: .8em; }
#nav li {display: inline;}
#nav a { border-left: 1px solid #555; float: left; display: inline; padding: 0 7px 0; text-decoration: none;}
#nav a:hover, #nav a:focus, #nav a.active { background: rgba(255,255,255,.3); }
#nav a.active { text-decoration: underline; }
 
#latest-snippet { -moz-box-shadow: 0 0 4px #AAA; -webkit-box-shadow: 0 0 4px #AAA; box-shadow: 0 0 4px #AAA; border: 1px dotted #999; padding: 20px; margin: 20px 0 0 20px; background: rgba(255,255,255,0.5); }
#content #latest-snippet h2 {background:none; font-size:1.4em; padding: 0 0 10px 0;}

#main {outline: 10px solid rgba(0,0,0,0.5);color: #EEE;width: 770px; margin: 40px auto 20px;background: #181818;overflow: hidden;}
#content, #sidebar { }

#content { padding: 0 20px; margin: 0;}
#content.single { padding: 45px; margin: 0; width: auto;}

#content h2, #content h1, #content h3 { text-align: center; padding: 20px 20px 5px 5px; text-shadow: 0 0 2px #666; }
#content h1 {font-size: 1.8em;}
#content h1, #content h2.major { border-bottom: 2px solid #444; }
#content h2 {font-size: 1.6em;}
#content h3 {font-size: 1.4em;}

#content h1.inpage {background: none;}
#content h2 a {color: #EEE;border: none;text-decoration: none;}
#content h2.archive {background:none;font-size:2.2em;}
#content .main-img {float: right; margin: 10px 0 10px 10px; border: 5px solid #555;}
.content .main-img img {margin:0;}
small.author {color: #EEE;display: block; text-align: center;font-size: .7em; padding: 20px;padding-top: 8px;}
small.author a {color: #EEE;}
#content a {text-decoration: underline;}
#content a:hover,
#content a:focus,
#content a:active {background-color: #111;}
#content p, #content ul, #content .post-content ol {font-size: 1em;padding:0.4em 0 0.8em 0;line-height: 1.6em;}
#content ul ul {font-size:1em;}
#content ul p { font-size: 1em;}

#content #post-list { padding: 2em 0; }
#content #post-list li { font-size: 1.2em; overflow: hidden; }
#content #post-list li a { padding: 5px; float: left; width: 70%; }
#content #post-list li a:hover,
#content #post-list li a:focus { background: #333; color: #FFF; }
#content #post-list li .meta { padding: 5px; float: right; width: 25%; text-align: right; }

#content .post-content ol {padding: 0.6em 0 0.6em 30px;}
