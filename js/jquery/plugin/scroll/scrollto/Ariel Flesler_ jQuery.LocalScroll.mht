document.documentElement.className = 'js';
function amf_toggle( elem ){
	if( elem )
		elem.style.display = elem.style.display == 'block' ? '' : 'block';
}
function amf_next( elem, tag ){
	tag = tag.toUpperCase();
	do elem = elem && elem.nextSibling;
	while( elem && elem.nodeName != tag );
	return elem;
}
function amf_first( elem, tag ){
	elem = elem.firstChild;
	tag = tag.toUpperCase();
	return elem.nodeName != tag ? amf_next( elem, tag ) : elem;
}
function amf_togglePanel( panel ){
	panel = amf_next( panel, 'ul' );
	amf_toggle(panel);
};
function amf_link( text ){
	var a = document.createElement('a');			
	a.href = '#';
	a.appendChild(document.createTextNode(text));
	return a;
}
function amf_toggle_text( elem, one, two ){
	var t = elem.firstChild;
	t.nodeValue = t.nodeValue == one ? two : one;
}
