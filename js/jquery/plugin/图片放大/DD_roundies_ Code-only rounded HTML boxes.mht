				<div id=3D"tests">
					<h3><a name=3D"background_properties">CSS Background Properties (re=
levant for IE6)</a></h3>
					<p>Use these fields to change background properties.</p>
					<p>The last field is a text field and will accept 'red' or '#F00' o=
r '#FF0000'.</p>
					<div id=3D"background_conditions_test" class=3D"png_bg box">
						<select id=3D"change_background_position" rel=3D"Position">
							<option>left top</option>
							<option>left center</option>
							<option>left bottom</option>
							<option>right top</option>
							<option>right center</option>
							<option>right bottom</option>
							<option>center top</option>
							<option>center center</option>
							<option>center bottom</option>
							<option>20% top</option>
							<option>20% center</option>
							<option>20% bottom</option>
							<option>20px 30px</option>
							<option>-20px -30px</option>
						</select>
						<select id=3D"change_background_repeat" rel=3D"Repeat">
							<option>repeat</option>
							<option>no-repeat</option>
							<option>repeat-x</option>
							<option>repeat-y</option>
						</select>
						<input id=3D"change_background_color" rel=3D"Color" type=3D"text" =
value=3D"#FF0000" />
					</div>
					<script src=3D'http://www.dillerdesign.com/pics/js/DD_Base.js'></sc=
ript>
					<script>
						var test =3D DD.dom.id('background_conditions_test');
						var setup =3D function() {
							DD.event.add(this, 'change', function() {
								var options =3D this.getElementsByTagName('option');
								var rel =3D this.getAttribute('rel');
								var self =3D this;
								DD.fade(test, false, {time:100, end:function() {
									test.style['background' + rel] =3D options[self.selectedIndex].=
innerHTML;
									DD.fade(test, true, {time:100})
								}});
							});
						};
						var selects =3D {position: true, repeat: true};
						for (var s in selects) {
							setup.call(DD.dom.id('change_background_' + s));
						}
						var changeBgColor =3D function() {							=

							try {
								test.style.backgroundColor =3D this.value;
							} catch(err) {}
						};
						var bgColorTextField =3D DD.dom.id('change_background_color');
						DD.event.add(bgColorTextField, 'keyup', changeBgColor);
						changeBgColor.call(bgColorTextField);
					</script>
					<hr>
					<h3><a name=3D"animation">Javascript Animation</a></h3>
					<div id=3D"morph" class=3D"png_bg box">
						Click on me.
					</div>
					<script>
						var d =3D 0;
						var c =3D 0;
						var upOrDown =3D true;
						var setBorder =3D function() {
							var obj =3D this;
							var windDown =3D setInterval(function() {
								d +=3D upOrDown ? 1 : (-1);
								if (d >=3D 0 && d <=3D 10) {
									obj.style.borderWidth =3D d + 'px';
									c =3D d%11;
									if (c =3D=3D 10) {
										c =3D 'A';
									}
									c =3D c.toString();
									obj.style.marginLeft =3D d + 'px';
									obj.style.marginRight =3D d + 'px';
									obj.style.backgroundColor =3D '#' + c + c + c;
									obj.style.fontSize =3D (100 + (d*5)) + '%';
								}
								else {
									upOrDown =3D !upOrDown;
									clearInterval(windDown);
								}
							}, 10);
						};
						var morph =3D DD.dom.id('morph');
						DD.event.add(morph, 'click', function() {
							setBorder.call(morph);
						});
					</script>
