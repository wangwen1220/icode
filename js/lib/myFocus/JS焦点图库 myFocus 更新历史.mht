html{ background:#ffffff url(background.png) repeat-x 0 0; }
body{ background:#ffffff url(background.png) repeat-x 0 0; }
#hd .wp{ padding: 0;}
#nv{ background-color:#616378;background-image:url(nv.png); }
	#nv li,#nv li a:hover,#nv li.hover a,#nv li.hover a:hover{ background-image:url(nv_a.png)!important; }
		#nv li.a{ background-color:#818598;}

/**/
.fl{ border-color:#818598; font-family: Verdana;}
.bm_h,.tl th,.tl td{ border-bottom-color:#CDCDCD; }
.bm_h, .forum_rjob .frame-title { background: none repeat scroll 0 0 #DDDCE4; border-bottom: 1px solid #B6B6C0; border-top: 1px solid #EEECF6; }
.bmw .bm_h { background: none repeat scroll 0 0 #818598; border: 1px solid #818598; border-top-color: #818598; }
	#nv_group .bmw .bm_h{ border-width: 0;}
.sd .bm_h { background: none repeat scroll 0 0 #F2F2F2; }
#nv_misc .bm_h { background: none repeat scroll 0 0 #F2F2F2; border-top: 1px solid #FFFFFF; }

/*  */
.chart{background:url(chart.png) no-repeat scroll 0 50% transparent; }
#scbar, #scbar_form, .scbar_icon_td, .scbar_txt_td, .scbar_type_td, .scbar_btn_td{ background-image:url(search.png); }

/*  */
.bmw .bm_h,.bmw .bm_h a{ background-color:F2F2F2;color:#fff; }
.fl_tb{ color:#777; }
	.fl_tb a{ color:#000080; }
#scbar_btn .xi2{ color:#666; }

/*  */
.pgs #newspecial, .pgs #newspecialtmp{ float:left;background:url(pn_post.png) no-repeat 0 0;}
#post_reply,#post_replytmp{ float:left;background:url(pn_reply.png) no-repeat 0 0;}
#newspecial img,#newspecialtmp img,#post_reply img,#post_replytmp img{ opacity:0;filter:alpha(opacity=0);}
.moodfm_btn{ background-image:url(mood_input_btn.png)!important;}
#create_group_btn{ display:block;background:url(create_group.png) no-repeat 0 0;}
#create_group_btn img{ opacity:0;filter:alpha(opacity=0);}
.el .ec .hot{ background-image:url(hotspot.gif);}
html #case_tb_h .special,html #case_tb_h .special a,html #pages_tb_h .special,html #pages_tb_h .special a{ background:url(pnp2.png) no-repeat scroll 0 0 transparent;}
.pn,.tb .o, .tb .o a{ background-image:url(pn.png); }
.pnc,a.pnc, .tb .o{ border-color:#be5703; }

/*  */
.ct2_a,.ct3_a{ background: #ffffff url(vlineb.png) repeat-y 0 0; }
#nv_portal.pg_portalcp .ct2_a_r, #nv_forum.pg_post .ct2_a_r { background-image:none; }

/* tab */
.tb a{ background:none repeat scroll 0 0 #f2f2f7; }

/*forum_list*/
#threadlist .bm_c{ padding:0; }
.fl_tb td { padding: 4px 0; }

/*forum_viewthread*/
.pls{ background:none repeat scroll 0 0 #F2F2F7; border-right:1px solid #e1e1e1;}
	.ad .pls{ background:none repeat scroll 0 0 #c7cde5; border-right:1px solid #e1e1e1;}
	.ad .plc{ background:none repeat scroll 0 0 #e8ebf8; }
.exfm { background: none repeat scroll 0 0 #F2F2F7; border: 1px dashed #cdcdcd; }
html .forum_rjob .frame-title { background: none repeat scroll 0 0 #DDDCE4; border-bottom: 1px solid #B6B6C0; border-top: 1px solid #EEECF6; }
.fl .notlast { margin-bottom: 1px; }
html .fl_row td { border-top: 1px solid #B6B6C0; }

