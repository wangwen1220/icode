Content-Disposition: inline; filename=style.css
Content-Type: text/css; name=style.css
Content-Location: http://fed.renren.com/wp-content/themes/fednew/style.css
Content-Transfer-Encoding: 8bit

body,ul,li,img{padding:0;margin:0;border:0;}
ul,ol{list-style-type:none;}
body{
	font-size:14px;
	font-family:Helvetica,Arial,sans-serif;
}
a{
	color:#096dbb;
	text-decoration:none;
}
a:hover{
	text-decoration:underline;
}
.input-button,.input-submit{background-color:#005eac;border-color:#b8d4e8 #124680 #124680 #b8d4e8;border-style:solid;border-width:1px;color:#fff;cursor:pointer;font-size:12px;padding:2px 15px;text-align:center}
/* clearFix*/
.clearfix:after,.feed:after,.feed-content .headline:after,.comment:after{content:".";display:block;height:0;clear:both;visibility:hidden}
.clearfix,.feed,.feed-content .headline,.comment{display:inline-block}
.clearfix,.feed,.feed-content .headline,.comment {display:block}


.top{background:#2d2d2d url(images/top-bg.gif)  center bottom no-repeat;height:140px;}
.top-content{width:960px;margin:0 auto;}
.top-content .fed-logo{height:140px;text-indent:-9999px;width:230px;float:left;}
.top-content .fed-logo a{
width:230px;
height:140px;
display:block;
background:url(images/fed-logo.gif) 0 38px no-repeat;
}

.top-content .fed-logo a:hover{
background:url(images/fed-logo-hover.gif) 0 38px no-repeat;
}
.top-menu{
	float:right;
	height:45px;
	margin-right:35px;
	margin-top:94px;
	display:inline;
}
.top-menu a{color:#ffffff;text-decoration:none;}
.top-menu a:hover{border-bottom:3px solid #ffffff;text-decoration:none;}
.top-menu li{
	float:left;
	font-size:18px;
	padding:0 20px;
}

.top-menu li a{
	display:block;
}
.content-area{
	width:960px;
	margin:0 auto;
	background:url(images/bg.gif) right 0 repeat-y;
	padding-bottom:10px;
}
.post-title-area{
	background:url(images/title-bg.gif) 0 center no-repeat;
}
.post-title{
line-height:30px;
padding-left:37px;
overflow:hidden;
}

.post-title a{
	color:#096dbb;
	font-family:"Microsoft Yahei",Helvetica,Tahoma,Verdana,simsun,sans-serif;
	text-decoration:none;
	font-size:26px;
	font-weight:700;
}
.post-title a:hover{
	color:#e83928;
	text-decoration:none;
}
.post-meta{
	color:#4c4c4c;
	font-family:Helvetica,Tahoma,Verdana,simsun,sans-serif;
	font-size:14px;
	padding-left:37px;
line-height:25px;
}
.entry{
	padding-left:37px;
	line-height:1.5;
}
.post-bottom{
	padding-left:37px;
	color:#4c4c4c;
	background:url(images/post-bottom.gif) 0 center no-repeat;
	height:34px;
	line-height:34px;
}
#content{
	width:660px;
	float:left;
	padding-top:10px;
overflow:hidden;
}
.post{
	margin-top:25px;
	padding-bottom:30px;
	background:url(images/post-line.gif) 38px bottom no-repeat;
}
#sidebar{
	width:221px;
	float:right;
	margin-right:23px;
	display:inline;
	padding:14px;
}
.widget_categories h2{
	background:url(images/item1.gif) 0 bottom no-repeat;
	height:31px;
	text-indent:-9999px;
}

.widget_tag_cloud h2{
	background:url(images/tags.gif) 0 bottom no-repeat;
	height:31px;
	text-indent:-9999px;
}

.widget_recent_entries h2{
	background:url(images/new-post.gif) 0 bottom no-repeat;
	height:31px;
	text-indent:-9999px;
}

.widget_recent_comments h2{
	background:url(images/new-re.gif)  0 bottom no-repeat;
	height:31px;
	text-indent:-9999px;
}

#linkcat-2 h2{
	background:url(http://fed.renren.com/wp-content/uploads/2010/05/link1.gif) 0 bottom no-repeat;
	height:31px;
	text-indent:-9999px;
}

#linkcat-25 h2{
	background:url(http://fed.renren.com/wp-content/uploads/2010/05/link2.gif) 0 bottom no-repeat;
	height:31px;
	text-indent:-9999px;
}

.widget_recent_entries li,.wp_widget_mylinkorder li{
	background:url(images/line1.gif) 0 bottom repeat-x;
	padding:5px 0;
}

.widget_recent_entries li a,.wp_widget_mylinkorder li a,.widget_categories li a{
	color:#5c5c5c;
	display:block;
	width:100%;
}
.widget_tag_cloud a{color:#5c5c5c;}

.widget_categories li{
	height:23px;
	line-height:23px;
}

.navigation{
	padding-left:40px;
}

#respond,#comments{
	margin-left:37px;
}

.comment-input{
	width:580px;
	padding:5px;
	font-size:14px;
	font-family:Helvetica,Tahoma,Verdana,simsun,sans-serif;
}

.fn{
	font-style:normal;
}

.comment {
border-left:4px solid #005eac;
margin-bottom:20px;
padding:10px;
position:relative;
background-color:#e6f0f8;
}
.comment-meta{position:absolute;right:10px;top:17px;display:none;}



.commentlist .fn{font-style:normal;}

.commentlist .fn a{font-style:normal;}

.photo{margin-right:13px;}

.comment-body p{margin-left:50px;margin-top:7px;margin-bottom:7px;}

.comment-childs  .photo{float:left;}

.thdrpy{font-size:12px;}

.chalt {
background-color:#464646;
position:relative;
}
.comment-childs {
background-color:#ededed;
border:1px solid #b1b1b1;
margin-left:50px;
padding:10px;
margin-bottom:10px;
}

#footer{
	background:url(images/bottom-bg.gif) 0 0 repeat-x;
	height:93px;
}

#footer .copyright{
	width:960px;
	margin:0 auto;
	background:url(images/copyright.gif) 0 0 no-repeat;
	height:93px;
	text-indent:-9999px;	
}

.code-holder{
border-left:8px solid #999999;
padding-left:10px;
font-family:"Courier New",sans-serif;
background-color:#e8e8e8;
}

.pingback{
background-color:#ededed;
border-left:4px solid #666666;
margin-bottom:20px;
padding:10px;
position:relative;
}
